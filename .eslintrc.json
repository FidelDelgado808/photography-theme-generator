{
    "env": {
      "browser": true,
      "es2021": true,
        "jest": true
  
    },
    "extends": [
      "plugin:react/recommended",
      "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        }
      }
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }  
  }